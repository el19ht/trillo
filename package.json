{
  "name": "trillo",
  "version": "1.0.0",
  "description": "flexbox project",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/style.scss css/style.css -w",
    "compile:sass": "node-sass -o sass/style.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.concat.css -o css/style.prefix.css"
  },
  "author": "stradiuarius",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-uglify": "^3.0.2",
    "normalize.css": "^8.0.1"
  },
  "dependencies": {
    "magnific-popup": "^1.1.0",
    "slick-carousel": "^1.8.1"
  }
}
